generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DB_FILE")
}

model Answer {
  AnswerText String?
  SurveyID   Int?
  UserID     Int?
  QuestionID Int?
  id         Int     @id @default(autoincrement())
}

model Question {
  id           Int     @id @default(autoincrement())
  questiontext String?
}

model Survey {
  SurveyID    Int     @id @default(autoincrement())
  Description String?
}
